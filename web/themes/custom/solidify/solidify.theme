<?php

/**
 * @file
 * Functions to support theming in the solidify theme.
 */

function solidify_theme_suggestions_breadcrumb_alter(array &$suggestions, array $variables) {
  $suggestions[] = "breadcrumb__solidify";
}

function solidify_theme_suggestions_pager_alter(array &$suggestions, array $variables) {
  $suggestions[] = "pager__news";
}

function solidify_theme_suggestions_form_element_alter(array &$suggestions, array $variables) {
  $suggestions[] = "form__exposed_tags";
}
